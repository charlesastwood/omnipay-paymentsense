    "keywords": [
        "payment sense",
        "paymentsense",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "driver"
    ],
    "homepage": "https://github.com/digitickets/omnipay-paymentsense",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Coates",
            "email": "andycoatz@gmail.com"
        }
    ],
    "autoload": {
