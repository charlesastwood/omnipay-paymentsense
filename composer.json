    "keywords": [
        "payment sense",
        "paymentsense",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "driver"
    ],
    "homepage": "https://github.com/charlesastwood/omnipay-paymentsense",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Astwood",
            "email": "charles@medialam.com"
        }
    ],
    "autoload": {
        "psr-4": { "Medialam\\PaymentSense\\" : "src/" }
    },
    "require": {
        "omnipay/common": "3.*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "guzzle/plugin-mock": "~3.1",
