{
    "name": "charlesastwood/omnipay-paymentsense",
    "type": "library",
    "description": "PaymentSense driver for the Omnipay payment processing library",
    "keywords": [
        "payment sense",
        "paymentsense",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "driver"
    ],
    "homepage": "https://github.com/charlesastwood/omnipay-paymentsense",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Astwood",
            "email": "charles@medialam.com"
        }
    ],
    "autoload": {
        "psr-4": { "Medialam\\PaymentSense\\" : "src/" }
    },
    "require": {
        "omnipay/common": "^3",
        "ext-simplexml": "*",
    },
    "require-dev": {
        "guzzle/plugin-mock": "^3",
        "mockery/mockery": "~1.3",
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}